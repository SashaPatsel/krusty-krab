<div class="main-content">
<div class="row">
   <div class="col-md-6 contain">
<h2>Uneaten Patties:</h2>

<ul>
  {{#each krabby}}
    {{#unless devoured}}
      {{> patty-block devoured=true}}
    {{/unless}}
  {{/each}}
</ul>

 </div>
  <div class="col-md-5">
<h2>Devoured Patties:</h2>

<ul>
  {{#each krabby}}
    {{#if devoured}}
      {{> patty-block devoured=false}}
    {{/if}}
  {{/each}}
</ul>
</div>
</div>
<br><br><br><br><br><br>
<div class="contain">
<h2>Add a Patty</h2>
<form class="create-form">

  <div class="form-group">
    <label for="krabby">Krabby Patty:</label>
    <input  class="add-butto" type="text" id="krabby" name="name">
  </div>

  <button class="add-butto" type="submit">Add Krabby Patty</button>
</form>
 </div> 
</div>
